*
    margin 0
    padding 0
body
    font-family "微软雅黑","Microsoft YaHei",sans-serif;
    font-size 14px
    color #666
a
    text-decoration none 
    color #666
input,select 
    outline none
i,em,b,strong
    font-style normal
    font-weight normal
li 
    list-style none 
img 
    border none
.clearfix:after 
    content ""
    display block 
    clear both
.clearfix 
    *zoom 1 

h1,h2,h3,h4,h5
    font-weight normal

